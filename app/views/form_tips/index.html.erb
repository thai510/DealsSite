<%= content_for :rightlayout do %>
<div class="leftSideLayout">
<h1>Form Tips</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @form_tips.each do |form_tip| %>
  <tr>
    <td class="formTipLink"><%= form_tip.title %></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td class="formTipLink"><%= link_to 'Edit', edit_form_tip_path(form_tip) %></td>
    <td></td>
    <td></td>
    <td></td>
    <td class="formTipLink"><%= link_to 'Destroy', form_tip, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<div class="blueGreenLink"><%= link_to 'Add Form tip', new_form_tip_path %></div>
</div>
<% end %>
