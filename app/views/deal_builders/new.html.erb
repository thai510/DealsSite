<head>
<%= stylesheet_link_tag 'style.css' %>
</head>

<% unless params[:deal_builder_exist] then %>
<%= render 'db_step_ones/form', :object => (@db_step_one = DbStepOne.new), :object => @deal_builder%>
<% else %>
  <% unless DealBuilder.find(params[:deal_builder_exist]).db_step_two then %>
    <%= render 'db_step_twos/form', :object => (@db_step_two = DbStepTwo.new), 
                                    :object => (@deal_builder = DealBuilder.find(params[:deal_builder_exist])) %>
  <% else %>
    <% unless DealBuilder.find(params[:deal_builder_exist]).db_step_three then %>
      <%= render 'db_step_threes/form', :object => (@db_step_three = DbStepThree.new),
                                        :object => (@deal_builder = DealBuilder.find(params[:deal_builder_exist])) %>
    <% else %>
      <% unless DealBuilder.find(params[:deal_builder_exist]).db_step_four then %>
      <%= render 'db_step_fours/form', :object => (@db_step_four = DbStepFour.new),
                                        :object => (@deal_builder = DealBuilder.find(params[:deal_builder_exist])) %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
