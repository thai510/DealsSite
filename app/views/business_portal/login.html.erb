<% unless session[:business_id] %>
 <h1> Business Login </h1>
 <br />
  <%= form_tag do %>
     <%= text_field_tag :email, params[:email], :placeholder => "Email" %>
     <%= password_field_tag :password, params[:password], :placeholder => "Password"%>
     <%= submit_tag 'Login'%>
  <% end %>
<% else %>
 Welcome <%= Business.find(session[:business_id]).name %>
 <%= link_to 'Logout', logout_business_path, :method => :delete %>
<% end %>
